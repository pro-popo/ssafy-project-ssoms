AA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,cAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,QAAA,CAAA,SAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OARA;AASA,KAvBA;AAwBA,IAAA,gBAxBA,8BAwBA;AACA,MAAA,KAAA,CACA,IADA,CACA,MAAA,CAAA,GAAA,GAAA,MAAA,CAAA,MAAA,CAAA,gBADA,EACA,KAAA,UADA,EAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,SAAA,EAAA;AACA,UAAA,KAAA,CAAA,cAAA,CAAA;AACA,SAFA,MAEA,IAAA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,MAAA,EAAA;AACA,UAAA,KAAA,CAAA,cAAA,CAAA;AACA;AACA,OARA,EASA,KATA,CASA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OATA;AAUA,KAnCA;AAoCA,IAAA,IApCA,kBAoCA;AAAA;;AACA,MAAA,KAAA,CACA,GADA,CACA,MAAA,CAAA,GAAA,GAAA,aADA,EAEA,IAFA,CAEA,UAAA,GAAA;AAAA,eAAA,MAAA,CAAA,UAAA,GAAA,GAAA;AAAA,OAFA;AAGA;AAxCA,GAbA;AAuDA,EAAA,OAvDA,qBAuDA;AACA,SAAA,mBAAA;AACA;AAzDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <h2 class=\"mb-3\">Oracle DB설정</h2>\r\n    <v-btn color=\"error\" @click=\"test\">TEST</v-btn>\r\n    <div class=\"zz\">\r\n      {{ testString }}\r\n    </div>\r\n    <form>\r\n      <div class=\"input-group-oracle\">\r\n        <span class=\"setting-oracle-font\">Oracle URL</span>\r\n        <v-text-field\r\n          label=\"연동할 Oracle URL을 입력해주세요.\"\r\n          v-model=\"oracleData.oracleURL\"\r\n        ></v-text-field>\r\n      </div>\r\n      <div class=\"input-group-oracle\">\r\n        <span class=\"setting-password-font\">Password</span>\r\n        <v-text-field\r\n          v-model=\"oracleData.oraclePassword\"\r\n          :type=\"'password'\"\r\n          name=\"input-10-1\"\r\n          label=\"Oracle Password를 입력해주세요.\"\r\n        ></v-text-field>\r\n      </div>\r\n      <hr class=\"oracle-line\" />\r\n      <div class=\"setting-storage\">\r\n        <span class=\"setting-right\" style=\"display:inline-block; width: 350px\"\r\n          >Oracle ID\r\n          <v-text-field\r\n            label=\"Oracle ID를 입력해주세요.\"\r\n            v-model=\"oracleData.oracleID\"\r\n          ></v-text-field>\r\n        </span>\r\n        <span style=\"display:inline-block; width: 350px\"\r\n          >Oracle SID\r\n          <v-text-field\r\n            label=\"오라클 SID을 입력해주세요.\"\r\n            v-model=\"oracleData.oracleSID\"\r\n          ></v-text-field>\r\n        </span>\r\n      </div>\r\n      <div class=\"setting-storage\">\r\n        <span class=\"setting-right\" style=\"display:inline-block; width: 350px\"\r\n          >데이터 갱신 주기\r\n          <v-text-field label=\"아직 X\"></v-text-field>\r\n        </span>\r\n        <span style=\"display:inline-block; width: 350px\"\r\n          >데이터 배치 시각\r\n          <v-text-field label=\"아직 X\"></v-text-field>\r\n        </span>\r\n      </div>\r\n    </form>\r\n    <v-btn\r\n      color=\"primary\"\r\n      class=\"setting-oracle-save-button1\"\r\n      @click=\"checkConOracleDB\"\r\n    >\r\n      연결\r\n    </v-btn>\r\n    <v-btn\r\n      color=\"primary\"\r\n      class=\"setting-oracle-save-button2\"\r\n      @click=\"setSettingsOracleDB\"\r\n    >\r\n      저장\r\n    </v-btn>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport SERVER from \"@/api/spring.js\";\r\n\r\nexport default {\r\n  name: \"OracleDB\",\r\n  data() {\r\n    return {\r\n      oracleData: {\r\n        oracleURL: \"\",\r\n        oracleID: \"\",\r\n        oraclePassword: \"\",\r\n        oracleSID: \"\"\r\n      },\r\n      testString: \"\"\r\n    };\r\n  },\r\n  methods: {\r\n    setSettingsOracleDB() {\r\n      axios\r\n        .post(SERVER.URL + SERVER.ROUTES.setSettingsOracleDB, this.oracleData)\r\n        .then((res) => {\r\n          if (res.data.result === \"success\") {\r\n            alert(\"저장에 성공하였습니다.\");\r\n          } else if (res.data.result === \"fail\") {\r\n            alert(\"저장에 실패하였습니다.\");\r\n          }\r\n        })\r\n        .catch((err) => console.log(err));\r\n    },\r\n    getSettingsOracleDB() {\r\n      axios\r\n        .get(SERVER.URL + SERVER.ROUTES.getSettingsOracleDB)\r\n        .then((res) => {\r\n          this.oracleData.oracleURL = res.data.map.oracleDB.oracleURL;\r\n          this.oracleData.oraclePassword = res.data.map.oracleDB.oraclePassword;\r\n          this.oracleData.oracleID = res.data.map.oracleDB.oracleID;\r\n          this.oracleData.oracleSID = res.data.map.oracleDB.oracleSID;\r\n        })\r\n        .catch((err) => console.log(err));\r\n    },\r\n    checkConOracleDB() {\r\n      axios\r\n        .post(SERVER.URL + SERVER.ROUTES.checkConOracleDB, this.oracleData)\r\n        .then((res) => {\r\n          if (res.data.result === \"success\") {\r\n            alert(\"연결에 성공하였습니다.\");\r\n          } else if (res.data.result === \"fail\") {\r\n            alert(\"연결에 실패하였습니다.\");\r\n          }\r\n        })\r\n        .catch((err) => console.log(err));\r\n    },\r\n    test() {\r\n      axios\r\n        .get(SERVER.URL + \"/admin/test\")\r\n        .then((res) => (this.testString = res));\r\n    }\r\n  },\r\n  created() {\r\n    this.getSettingsOracleDB();\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.setting-oracle-font {\r\n  font-size: 20px;\r\n  margin-right: 30px;\r\n}\r\n.setting-password-font {\r\n  font-size: 20px;\r\n  margin-right: 40px;\r\n}\r\n.setting-storage {\r\n  font-size: 20px;\r\n  margin-top: 40px;\r\n}\r\n.setting-right {\r\n  margin-right: 120px;\r\n}\r\n.setting-oracle-save-button1 {\r\n  margin-top: 50px;\r\n  margin-right: 50px;\r\n  padding: 15px 20px !important;\r\n  font-size: 15px !important;\r\n  float: right;\r\n}\r\n.setting-oracle-save-button2 {\r\n  margin-top: 50px;\r\n  margin-right: 10px;\r\n  padding: 15px 20px !important;\r\n  font-size: 15px !important;\r\n  float: right;\r\n}\r\n.input-group-oracle {\r\n  height: 60px;\r\n  line-height: 60px;\r\n  display: flex;\r\n}\r\n.oracle-line {\r\n  margin-top: 25px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/admin"}]}