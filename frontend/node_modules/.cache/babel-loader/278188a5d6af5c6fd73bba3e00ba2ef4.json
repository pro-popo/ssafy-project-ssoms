ACA,SAFA,MAEA,IAAA,GAAA,CAAA,IAAA,CAAA,MAAA,IAAA,kBAAA,EAAA;AACA,UAAA,KAAA,CACA,uCADA,CAAA;AAGA,SAJA,MAIA,IAAA,GAAA,CAAA,IAAA,CAAA,MAAA,IAAA,mBAAA,EAAA;AACA,UAAA,KAAA,CACA,yCADA,CAAA;AAGA;AACA,OAtBA;AAuBA;AAzBA,GAjBA;AA4CA,EAAA,KAAA,EAAA;AACA,IAAA,YAAA,EAAA,OADA;AAEA,IAAA,iBAAA,EAAA;AAFA,GA5CA;AAiDA,EAAA,KAAA,EAAA;AACA,IAAA,YAAA,EAAA,wBAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAHA;AAIA,IAAA,MAAA,EAAA,kBAAA;AACA,UAAA,KAAA,OAAA,CAAA,KAAA,IAAA,EAAA,IAAA,KAAA,OAAA,CAAA,EAAA,IAAA,EAAA,EAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,eAAA;AACA;AACA;AATA,GAjDA;AA4DA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aACA,KAAA,aAAA,IAAA,gDADA;AAGA,KALA;AAMA,IAAA,cANA,4BAMA;AACA,aAAA,KAAA,OAAA,CAAA,EAAA,KAAA,KAAA,SAAA,IAAA,wBAAA;AACA;AARA;AA5DA,CAAA","sourcesContent":["<template>\r\n  <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n    <v-card>\r\n      <v-card-title class=\"delete-headline\">\r\n        <span style=\"margin-left:10px; color:var(--font-sub2-color)\">\r\n          Delete Member\r\n        </span>\r\n        <v-btn icon @click=\"dialog = !dialog\">\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n      </v-card-title>\r\n      <v-divider></v-divider>\r\n      <v-card-text style=\"padding-top:20px; padding-left:30px\">\r\n        <h2\r\n          style=\"font-size:20px; padding:15px 0px 10px 0px ;  word-break:keep-all\"\r\n        >\r\n          {{ deleteMemberEmail }} 님의 계정을\r\n        </h2>\r\n        <h2 style=\"font-size:20px;\">영구적으로 삭제하시겠습니까?</h2>\r\n\r\n        <p style=\"margin-top:30px\">\r\n          계정 삭제를 원하신다면, 당신의 이메일과 패스워드를 입력해주세요.\r\n        </p>\r\n        <v-form ref=\"form\" class=\"delete-input\">\r\n          <v-text-field\r\n            prepend-icon=\"mdi-email\"\r\n            v-model=\"account.email\"\r\n            :rules=\"[rules.required]\"\r\n            label=\"E-mail\"\r\n            required\r\n          ></v-text-field>\r\n          <v-text-field\r\n            prepend-icon=\"mdi-lock\"\r\n            v-model=\"account.pw\"\r\n            :rules=\"[rules.required, emailMatch]\"\r\n            :type=\"'password'\"\r\n            name=\"input-10-1\"\r\n            label=\"Password\"\r\n          ></v-text-field>\r\n          <v-text-field\r\n            v-model=\"confirmPw\"\r\n            prepend-icon=\"mdi-lock-check\"\r\n            :rules=\"[rules.required, pwConfirmRules]\"\r\n            :type=\"'password'\"\r\n            name=\"input-10-1\"\r\n            label=\"Confirm Password\"\r\n            @click:append=\"show1 = !show1\"\r\n          ></v-text-field>\r\n        </v-form>\r\n      </v-card-text>\r\n      <v-divider></v-divider>\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <v-btn text color=\"var(--main-color)\" @click=\"deleteMember\"\r\n          >Submit</v-btn\r\n        >\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport SERVER from \"@/api/spring.js\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"DeleteUserByAdmin\",\r\n  data() {\r\n    return {\r\n      dialog: false,\r\n\r\n      account: {\r\n        email: \"\",\r\n        pw: \"\"\r\n      },\r\n      confirmPw: \"\",\r\n      rules: {\r\n        required: (v) => !!v || \"Required.\"\r\n      },\r\n      deleteSuccess: true\r\n    };\r\n  },\r\n  methods: {\r\n    deleteMember() {\r\n      axios\r\n        .post(SERVER.URL + SERVER.ROUTES.deleteMember, {\r\n          adminemail: this.account.email,\r\n          deleteemail: this.deleteMemberEmail,\r\n          pw: this.account.pw\r\n        })\r\n        .then((res) => {\r\n          if (res.data.result == \"success\") {\r\n            alert(\"회원 계정을 성공적으로 삭제하였습니다.\");\r\n            this.dialog = false;\r\n            this.$emit(\"delete-member-admin\");\r\n          } else if (res.data.result == \"wrong_admin_pw\") {\r\n            this.deleteSuccess = false;\r\n          } else if (res.data.result == \"wrong_user_email\") {\r\n            alert(\r\n              \"삭제할 계정이 존재하지 않습니다. 새로고침 후, 다시 시도해주세요.\"\r\n            );\r\n          } else if (res.data.result == \"wrong_admin_email\") {\r\n            alert(\r\n              \"현재 계정은 관리자 권한이 없습니다. 새로고침 후, 다시 시도해주세요.\"\r\n            );\r\n          }\r\n        });\r\n    }\r\n  },\r\n  props: {\r\n    dialogDelete: Boolean,\r\n    deleteMemberEmail: String\r\n  },\r\n\r\n  watch: {\r\n    dialogDelete: function() {\r\n      this.dialog = true;\r\n    },\r\n    dialog: function() {\r\n      if (this.account.email != \"\" || this.account.pw != \"\") {\r\n        this.$refs.form.reset();\r\n        this.$refs.form.resetValidation();\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    emailMatch() {\r\n      return (\r\n        this.deleteSuccess || \"The email and password you entered don't match\"\r\n      );\r\n    },\r\n    pwConfirmRules() {\r\n      return this.account.pw === this.confirmPw || \"Password do not match.\";\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/components/account/admin"}]}