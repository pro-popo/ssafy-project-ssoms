                  <RealTime />\r\n                  </div>\r\n                  <p style=\"margin-top:40px\">\r\n                    마지막으로, USER 계정으로 회원가입 및 로그인 진행 후 SSOMS를\r\n                    이용해주세요.\r\n                  </p>\r\n                  <v-btn\r\n                    style=\"margin-top:5px;\"\r\n                    @click=\"logout\"\r\n                    color=\"black\"\r\n                    dark\r\n                  >\r\n                    <span style=\"color:white\">Login</span>\r\n                    <v-icon style=\"margin-left:10px;color:white\" size=\"20\"\r\n                      >mdi-arrow-right</v-icon\r\n                    ></v-btn\r\n                  >\r\n                </div>\r\n              </div>\r\n            </v-row>\r\n          </v-sheet>\r\n        </v-carousel-item>\r\n      </v-carousel>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport OracleDB from \"@/views/admin/OracleDB\";\r\nimport Schema from \"@/views/admin/Schema\";\r\nimport RealTime from \"@/views/admin/RealTime\";\r\n\r\nimport { mapActions } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      colors: \"transparent\",\r\n      model: 0\r\n    };\r\n  },\r\n  components: {\r\n    OracleDB,\r\n    Schema,\r\n    RealTime\r\n  },\r\n  methods: {\r\n    ...mapActions(\"Account\", [\"logout\"]),\r\n    oracleSave() {\r\n      console.log(\"this.model dur>?Sd,AMf;sda,fgsdafgasdgf\");\r\n      this.model = 2;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.setting-container {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-content: center;\r\n}\r\n.setting-menu {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  margin: 110px 0;\r\n  margin-left: -40px;\r\n}\r\n.setting-sheet {\r\n  padding-left: 50px;\r\n  height: 100%;\r\n}\r\n/* .setting-sheet * {\r\n  color: var(--font-main-color);\r\n} */\r\n.setting-sheet-step {\r\n  color: var(--font-s